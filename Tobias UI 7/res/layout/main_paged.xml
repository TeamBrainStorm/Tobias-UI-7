            <LinearLayout android:layout_width="match_parent" android:id="@+id/pageIndicator" android:layout_height="19dp" android:gravity="center">
            </LinearLayout>
            <JakedUp.AppDrawer.HorizontalPager android:id="@+id/horizontalPager1" android:layout_height="match_parent" android:layout_width="match_parent" android:layout_weight="1" android:layout_marginTop="5dp"></JakedUp.AppDrawer.HorizontalPager>

            <ImageView
                android:id="@+id/homeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
